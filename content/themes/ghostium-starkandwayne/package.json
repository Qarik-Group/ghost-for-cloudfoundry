{
  "name": "ghostium-starkandwayne",
  "version": "2.2.2",
  "description": "A Ghost theme focused on content based on Medium design/ux",
  "url": "http://ghostium.oswaldoacauan.com/",
  "copyright": "2013",
  "license": "MIT",
  "author": {
    "name": "Oswaldo Acauan",
    "email": "hello@oswaldoacauan.com",
    "url": "http://oswaldoacauan.com"
  },
  "config": {
    "posts_per_page": 6
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:oswaldoacauan/ghostium.git"
  },
  "bugs": {
    "url": "https://github.com/oswaldoacauan/ghostium/issues"
  },
  "keywords": [
    "ghost",
    "blog",
    "theme",
    "medium",
    "ghost-theme"
  ],
  "dependencies": {},
  "devDependencies": {
    "grunt": "*",
    "grunt-autoprefixer": "*",
    "grunt-contrib-clean": "*",
    "grunt-contrib-concat": "*",
    "grunt-contrib-copy": "*",
    "grunt-contrib-csslint": "*",
    "grunt-contrib-cssmin": "*",
    "grunt-contrib-imagemin": "*",
    "grunt-contrib-jshint": "*",
    "grunt-contrib-sass": "*",
    "grunt-contrib-uglify": "*",
    "grunt-conventional-changelog": "*",
    "grunt-csscss": "*",
    "grunt-csso": "*",
    "grunt-rev": "*",
    "grunt-svgmin": "*",
    "grunt-text-replace": "*",
    "grunt-usemin": "*",
    "matchdep": "*",
    "gscan": "*"
  },
  "engines": {
    "node": ">=10.0"
  },
  "scripts": {
    "build": "grunt build --force && cp build/default.hbs default.hbs && rm -rf assets/{js,css} && mkdir -p assets/{js,css} && cp build/assets/js/* assets/js && cp build/assets/css/*.css assets/css && gscan .",
    "test": "grunt --verbose",
    "postinstall": "bundle install && bower install"
  }
}
